{
	"manifest_version" : 2,
	"name" : "Delete History",
	"short_name" : "DH",
	"version" : "1.0",
	"author" : "<ISH>",
	"description" : "Тестовый пример работы расширения для Google Chrome в виде плагина удаления истории. By Lobanoff Artem 2020",
	
	"icons" : {
		"16" : "icons/16x16.png",
		"32" : "icons/32x32.png",
		"48" : "icons/48x48.png",
		"128" : "icons/128x128.png"
	},
	
	"permissions" : ["history", "notifications"],
	
	"background" : {
		"scripts" : ["background.js"],
		"persistent" : false
	},
	
    "browser_action": {
        "default_title": "Delete History",
        "default_icon": "icons/48x48.png"
		
    }
}


